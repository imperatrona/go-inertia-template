{
  "name": "go-inertia-template",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:dev:frontend\" \"npm:dev:server\"",
    "dev:frontend": "vite build --watch",
    "dev:server": "wgo run *.go",
